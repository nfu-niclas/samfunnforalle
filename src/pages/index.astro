---
import { useSanityClient } from '@sanity/astro';

const posts = await useSanityClient().fetch(`*[_type == "post"]`);
---

<html lang="no">
  <head>
    <title>Min side</title>
  </head>
  <body>
    <h1>Velkommen til Samfunn for Alle</h1>
    
    <ul>
      {posts.map((post: any) => (
        <li>
          <h2>{post.title}</h2>
          <p>{post.content}</p>
        </li>
      ))}
    </ul>
  </body>
</html>